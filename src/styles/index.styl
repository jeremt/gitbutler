
@import "sk.styl"

.sk-view
  margin auto
  max-width 960px

.sk-icon.icon-branch
  transform translateY(3px) rotate(-90deg)
  &:before
    content: "\e081"
  &:hover
    transform translateY(3px) rotate(-90deg) scale(1.2)
  &.selected
    color #fff
    transform translateY(3px) rotate(-90deg) scale(1.2)
